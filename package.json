{
  "name": "hibari",
  "version": "1.0.0",
  "description": "A Vue.js project",
  "author": "James Harris <wopian@wopian.me>",
  "private": true,
  "scripts": {
    "test": " ",
    "dev": "node build/dev-server.js",
    "build": "node build/build.js",
    "lint": "eslint --ext .js,.vue src",
    "log:major": "changelog -M",
    "log:minor": "changelog -m",
    "log:patch": "changelog -p"
  },
  "dependencies": {
    "buefy": "~0.6.2",
    "client-oauth2": "~4.1.0",
    "kitsu": "~4.1.0",
    "localforage": "~1.5.5",
    "vue": "~2.5.11",
    "vue-flickity": "^1.0.9",
    "vue-i18n": "~7.4.0",
    "vue-meta": "~1.4.0",
    "vue-router": "~3.0.1",
    "vue-timeago": "~3.4.2",
    "vue-typer": "~1.2.0",
    "vuex": "~3.0.1",
    "vuex-persist": "~1.1.0"
  },
  "devDependencies": {
    "autoprefixer": "~7.2.3",
    "babel-core": "~6.26.0",
    "babel-eslint": "~8.2.0",
    "babel-loader": "~7.1.2",
    "babel-plugin-istanbul": "~4.1.5",
    "babel-plugin-transform-runtime": "~6.23.0",
    "babel-plugin-webpack-chunkname": "^1.0.1",
    "babel-preset-env": "~1.6.1",
    "babel-preset-stage-0": "~6.24.1",
    "babel-register": "~6.26.0",
    "chalk": "~2.3.0",
    "connect-history-api-fallback": "~1.5.0",
    "copy-webpack-plugin": "~4.3.0",
    "css-loader": "~0.28.7",
    "cssnano": "~3.10.0",
    "eslint": "~4.15.0",
    "eslint-config-standard": "~10.2.1",
    "eslint-friendly-formatter": "~3.0.0",
    "eslint-loader": "~1.9.0",
    "eslint-plugin-html": "~4.0.1",
    "eslint-plugin-import": "~2.8.0",
    "eslint-plugin-node": "~5.2.1",
    "eslint-plugin-promise": "~3.6.0",
    "eslint-plugin-standard": "~3.0.1",
    "eventsource-polyfill": "~0.9.6",
    "express": "~4.16.2",
    "extract-text-webpack-plugin": "~3.0.2",
    "file-loader": "~1.1.5",
    "friendly-errors-webpack-plugin": "~1.6.1",
    "generate-changelog": "~1.7.0",
    "html-webpack-plugin": "~2.30.1",
    "http-proxy-middleware": "~0.17.4",
    "node-sass": "~4.7.2",
    "optimize-css-assets-webpack-plugin": "~3.2.0",
    "progress-bar-webpack-plugin": "~1.10.0",
    "pug": "~2.0.0-rc.4",
    "pug-loader": "~2.3.0",
    "rimraf": "~2.6.2",
    "sass-loader": "~6.0.6",
    "semver": "~5.4.1",
    "shelljs": "~0.8.0",
    "sw-precache-webpack-plugin": "~0.11.4",
    "tyu": "~1.0.1",
    "uglifyjs-webpack-plugin": "~1.1.4",
    "url-loader": "~0.6.2",
    "vue-loader": "~13.7.0",
    "vue-style-loader": "~3.0.3",
    "vue-template-compiler": "~2.5.11",
    "vue-test-utils": "~1.0.0-beta.8",
    "webpack": "~3.10.0",
    "webpack-bundle-size-analyzer": "~2.7.0",
    "webpack-dev-middleware": "~2.0.1",
    "webpack-hot-middleware": "~2.21.0",
    "webpack-merge": "~4.1.1",
    "webpack-pwa-manifest": "~3.4.0"
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 5.0.0",
    "yarn": ">= 1.0.0"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 ChromeAndroid versions",
    "last 2 Edge versions",
    "last 2 Firefox versions",
    "last 2 FirefoxAndroid versions",
    "last 2 Safari versions",
    "last 2 iOS versions"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,vue}"
    ],
    "coveragePathIgnorePatterns": [
      "src/(locales|api|store|router|main.js)"
    ],
    "coverageDirectory": "<rootDir>/test/coverage",
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "mapCoverage": true,
    "moduleNameMapper": {
      "=/([^\\.]*)$": "<rootDir>/src/components/$1.vue",
      "@/([^\\.]*)$": "<rootDir>/src/$1.vue"
    },
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "globals": {
      "NODE_ENV": "test"
    },
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".*\\.(vue)$": "vue-jest"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ]
  },
  "renovate": {
    "extends": [
      ":npm",
      ":preserveSemverRanges",
      ":combinePatchMinorReleases",
      ":renovatePrefix",
      ":semanticCommits",
      ":semanticCommitTypeAll(chore)",
      ":rebaseStalePrs",
      ":unpublishSafe",
      ":prNotPending",
      ":automergeDisabled",
      ":maintainLockFilesMonthly",
      ":ignoreUnstable",
      ":updateNotScheduled",
      ":label(dependencies)",
      ":timezone(Europe/London)",
      ":assignee(wopian)",
      "group:babelMonorepo",
      "group:jestMonorepo",
      "group:storybookMonorepo"
    ],
    "prTitle": "update {{depName}} to {{newVersion}}",
    "commitMessage": "update {{depName}} to {{newVersion}}",
    "baseBranch": "new",
    "packageRules": [
      {
        "packagePatterns": [
          "^webpack"
        ],
        "groupName": "webpack"
      },
      {
        "packagePatterns": [
          "^vue"
        ],
        "groupName": "vue"
      },
      {
        "packagePatterns": [
          "^vuex"
        ],
        "groupName": "vuex"
      }
    ]
  }
}
